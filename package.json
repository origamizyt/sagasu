{
  "name": "sagasu",
  "version": "0.1.0-beta",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "godev": "cd src-go && go run sagasu serve",
    "gobuild": "cd src-go && go generate && go build"
  },
  "dependencies": {
    "blakejs": "^1.2.1",
    "buffer": "^6.0.3",
    "remixicon": "^4.2.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "naive-ui": "^2.38.2",
    "npm-run-all2": "^6.1.2",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.11"
  }
}
